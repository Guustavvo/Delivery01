        <a href="/login">
            <div class="menu">
                <img src="/views/images/74472.png" alt="">
                <p>Minha conta</p>
            </div>
        </a>

        <a href="/loginAdmin">
            <div class="menu">
                <img src="/views/images/png-transparent-computer-icons-system-administrator-administrator-icon-silhouette-desktop-wallpaper-administrator-icon.png"
